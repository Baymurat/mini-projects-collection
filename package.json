  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "npm run clear && npm run mkdirs && npm run init-project && npm run modify",
    "clear": "rm -rf $npm_config_project",
    "mkdirs": "ts-node --esm ./generator/mkdirs.ts $npm_config_project $npm_config_react",
    "init-project": "cd ./$npm_config_project && npm init -y",
    "modify": "ts-node --esm ./generator/modify.ts $npm_config_project $npm_config_react"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
